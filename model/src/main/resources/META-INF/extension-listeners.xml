<?xml version="1.0" encoding="UTF-8"?>
<listeners>
<!--
    'entityName' é o nome da Entidade. É aqui que você define de qual entidade você ouvirá os eventos. Neste exemplo estamos ouvindo 'ItemNota'.
    O conteúdo de <listener> é o full qualified name da classe, ou seja, pacote + classe.
-->
    <listener entityName="ItemNota">br.com.fabricante.addon.exemplos.listeners.ExemploListener</listener>

</listeners>
